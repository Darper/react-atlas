{
  "name": "react-atlas",
  "version": "0.0.0",
  "description": "Composable React components with CSS Modules.",
  "author": "Digital River Experience Engineering (https://github.com/orgs/DigitalRiver/teams/experience-engineering)",
  "main": "./lib",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./server",
    "lint": "eslint ./ --ext .js,.jsx",
    "babelify": "babel ./components --out-dir ./lib",
    "build": "cross-env NODE_ENV=build npm run babelify && npm run build-css && rimraf ./lib/main.js",
    "build-css": "webpack --config webpack.config.buildcss.js",
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --no-single-run",
    "docs": "cross-env NODE_ENV=development rcs ./components/**/*.js -v -c ./docsConfig.js -d"
  },
  "bugs": {
    "url": "https://github.com/DigitalRiver/react-atlas/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "atlas",
    "CSS Modules",
    "components"
  ],
  "license": "MIT",
  "dependencies": {
    "blacklist": "^1.1.2",
    "classnames": "^2.2.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "normalize.css": "^3.0.3",
    "react-autosuggest": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-css-modules-transform": "^0.0.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-remove-statics": "^0.0.5",
    "bluebird": "^3.3.4",
    "css-modules-require-hook": "^4.0.0 ",
    "core-js": "^2.2.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.15.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.5",
    "phantomjs-polyfill": "^0.0.2",
    "postcss": "^5.0.19",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "7.1.3",
    "postcss-loader": "^0.8.2",
    "react-a11y": "^0.3.2",
    "react-addons-test-utils": "^0.14.7",
    "react-component-styleguide": "0.0.25",
    "react-docgen": "^2.7.0",
    "react-icons": "^1.0.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0",
    "npm-publish-nexus-cache-test": "0.0.1"
  },
  "repository": "https://github.com/DigitalRiver/react-atlas.git"
}
