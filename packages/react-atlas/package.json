{
  "name": "react-atlas",
  "publishConfig": {
    "registry": "http://nexus-master.digitalriverws.net/nexus/content/repositories/npm-internal/"
  },
  "version": "0.0.3",
  "description": "Composable React components with CSS Modules.",
  "author": "Digital River Experience Engineering (https://github.com/orgs/DigitalRiver/teams/experience-engineering)",
  "main": "./lib",
  "scripts": {
    "babelify": "babel ./src --out-dir ./lib && cat ../react-atlas-core/lib/reactAtlas.min.css ../react-atlas-default-theme/lib/reactAtlas.min.css > ./lib/reactAtlas.min.css",
    "build": "npm run babelify",
    "clean": "rimraf ./lib"
  },
  "bugs": {
    "url": "https://github.com/DigitalRiver/react-atlas/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "atlas",
    "CSS Modules",
    "components"
  ],
  "license": "MIT",
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-atlas-default-theme": "^0.0.3",
    "react-atlas-core": "^0.0.3",
    "normalize.css": "^4.1.1"
  },
  "repository": "https://github.com/DigitalRiver/react-atlas.git"
}
